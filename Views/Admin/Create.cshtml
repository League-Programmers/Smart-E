@page
@model Smart_E.Areas.Identity.Pages.CreateModel
@{
    ViewData["Title"] = "Create";
}
<div class="row">
    <div class="col-md-4">
        <form method="post">
            <div class="form-group">
                <label asp-for="Uploads.Date" class="control-label"></label>
                <input asp-for="Uploads.Date" class="form-control" />
                <span asp-validation-for="Uploads.Date" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="Uploads.Value" class="control-label"></label>
                <input asp-for="Uploads.Value" class="form-control" />
                <span asp-validation-for="Uploads.Value" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label for="formFileDisabled" class="form-label">Disabled file input example</label>
                <input class="form-control" type="file" id="formFileDisabled" disabled />
            </div>
        </form>
    </div>
</div>

<div>
    <a asp-page="Index">Back</a>
</div> 